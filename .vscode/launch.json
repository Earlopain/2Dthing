{
    "version": "0.1.0",
    "configurations": [
        {
            "name": ".NET Core Launch (console)",
            "type": "clr",
            "request": "launch",
            "preLaunchTask": "build",
            "program": "${workspaceRoot}/NESemu/bin/DesktopGL/AnyCPU/Debug/NESemu.exe",
            "cwd": "${workspaceRoot}/NESemu/bin/DesktopGL/AnyCPU/Debug/",
            "stopAtEntry": false,
            "externalConsole": false
        },
        {
            "name": ".NET Core Attach",
            "type": "clr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}